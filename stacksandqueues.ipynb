   "source": [
    "# List can be used as stacks\n",
    "a = [1, 2, 3]\n",
    "a.append(4)\n",
    "a.pop()\n",
    "\n",
    "# for using lists as queues - FIFO, use deque from collections\n",
    "from collections import deque\n",
    "\n",
    "queue = deque([1, 2, 3, 4])\n",
    "queue.append(5)\n",
    "queue.append(6)\n",
    "print(queue)\n",
    "\n",
    "queue.popleft()\n",
    "print(queue)"
   ]
